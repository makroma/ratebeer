<h1>Sign in</h1>

<p id="notice"><%= notice %></p>

<%= form_tag session_path do %>
	<label for="username">username</label>
   	<%= text_field_tag :username, params[:username] %>
  	<label for="password">password</label>
   	<%= password_field_tag :password, params[:password] %>
  	<%= submit_tag "Log in" %>
<% end %>